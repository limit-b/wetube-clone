extends ../main.pug
include ../mixins/avatar-mixin.pug
include ../mixins/video-mixin.pug

block content
    .user-profile__data
        +avatarMixin(userDB.avatarUrl, "profile")
        span= userDB.userName
    .shared__video-grid
        each item in userDB.userVideos
            +videoMixin(item)
        else
            span.shared__empty-message No videos found.
